   "source": [
    "# Problem Statement:\n",
    "# Write a function that reverses a string."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "The reversed string(using loops) is : olleh\n"
     ]
    }
   ],
   "source": [
    "def reverseString(s):\n",
    "    str = \"\"\n",
    "    for i in s:\n",
    "        str = i + str\n",
    "    return str\n",
    " \n",
    "s = input(\"Enter your string: \")\n",
    " \n",
    "print(\"The reversed string(using loops) is : \", end=\"\")\n",
    "print(reverseString(s))"
   ]
  }
